CREATE TABLE DEVELOPERS (
  ID INT PRIMARY KEY NOT NULL,
  NAME VARCHAR(20) NOT NULL,
  SURNAME VARCHAR(20) NOT NULL,
  AGE INT NOT NULL,
  SALARY REAL
);

CREATE TABLE SKILLS (
  SKILL_ID INT PRIMARY KEY NOT NULL,
  AREA_OF_SKILL VARCHAR(20) NOT NULL
);

CREATE TABLE PROJECTS (
  PROJECT_ID INT PRIMARY KEY NOT NULL,
  PROJECT_TITLE VARCHAR(50) NOT NULL
);

CREATE TABLE COMPANIES (
  COMPANY_ID INT PRIMARY KEY NOT NULL,
  COMPANY_NAME VARCHAR(20) NOT NULL,
  NUMBER_OF_EMPLOYEES INT
);

CREATE TABLE CUSTOMERS (
  CUSTOMER_ID INT PRIMARY KEY NOT NULL,
  CUSTOMER_NAME VARCHAR(20) NOT NULL
);

COMMIT;

ALTER TABLE DEVELOPERS ADD CONSTRAINT PROJECT_ID_FK FOREIGN KEY (PROJECT) REFERENCES PROJECTS (PROJECT_ID);

ALTER TABLE PROJECTS ADD CONSTRAINT COMPANY_ID_FK FOREIGN KEY (company_id) REFERENCES COMPANIES (COMPANY_ID);

ALTER TABLE PROJECTS ADD CONSTRAINT CUSTOMER_ID_FK FOREIGN KEY (customer_id) REFERENCES CUSTOMERS (CUSTOMER_ID);

ALTER TABLE SKILLS ADD CONSTRAINT DEVELOPER_ID_FK FOREIGN KEY (developer_id) REFERENCES DEVELOPERS (ID);

COMMIT


